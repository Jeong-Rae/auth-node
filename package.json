{
     "name": "@sejong-univ-api/auth-node",
     "version": "0.0.1",
     "description": "세종대학교 SSO을 통한 학생인증를 하는 라이브러리",
     "main": "dist/cjs/index.js",
     "module": "dist/esm/index.js",
     "types": "dist/types/index.d.ts",
     "exports": {
          "import": "./dist/esm/index.js",
          "require": "./dist/cjs/index.cjs"
     },
     "files": [
          "dist/cjs/",
          "dist/esm/",
          "dist/types/"
     ],
     "scripts": {
          "lint": "prettier --check . && eslint . --ext .js,.ts",
          "lint:fix": "prettier --write . && eslint . --ext .js,.ts --fix",
          "format": "prettier --write .",
          "format:check": "prettier --check .",
          "clean": "rimraf dist",
          "build:cjs": "tsc -p tsconfig.cjs.json",
          "build:esm": "tsc -p tsconfig.esm.json",
          "build:types": "tsc --emitDeclarationOnly --project tsconfig.types.json",
          "build": "npm run lint && npm run build:cjs && npm run build:esm && npm run build:types",
          "prepublishOnly": "npm run build",
          "clean-build": "npm run clean && npm run build",
          "test": "jest"
     },
     "keywords": [
          "sejong",
          "university",
          "sso",
          "auth"
     ],
     "publishConfig": {
          "access": "public"
     },
     "author": "Jeong-Rae",
     "license": "ISC",
     "type": "module",
     "devDependencies": {
          "@eslint/js": "^9.22.0",
          "@types/jest": "^29.5.14",
          "@typescript-eslint/eslint-plugin": "^8.26.1",
          "@typescript-eslint/parser": "^8.26.1",
          "eslint": "^9.22.0",
          "eslint-config-prettier": "^10.1.1",
          "eslint-import-resolver-typescript": "^3.9.1",
          "eslint-plugin-import": "^2.31.0",
          "eslint-plugin-node": "^11.1.0",
          "eslint-plugin-prettier": "^5.2.3",
          "eslint-plugin-promise": "^7.2.1",
          "jest": "^29.7.0",
          "prettier": "^3.5.3",
          "ts-jest": "^29.2.6",
          "typescript": "^5.8.2"
     },
     "repository": {
          "type": "git",
          "url": "https://github.com/sejong-univ-api/auth-node.git"
     },
     "bugs": {
          "url": "https://github.com/sejong-univ-api/auth-node.git/issues"
     },
     "homepage": "https://github.com/sejong-univ-api/auth-node.git#readme",
     "dependencies": {
          "cheerio": "^1.0.0",
          "rimraf": "^6.0.1"
     }
}
